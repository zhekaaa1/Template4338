<Window x:Class="Template4338.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Template4338"
        mc:Ignorable="d"
        Title="Window1" Height="200" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Label Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2">Евгений, 18</Label>
        <Button Grid.Row="1" Grid.Column="0" Margin="5" Click="ImportExcel_ButtonCliked">Импорт Excel</Button>
        <Button Grid.Row="1" Grid.Column="1" Background="Lime" Margin="5" Click="ExportExcel_Click">Экспорт Excel</Button>
        <Button Grid.Row="2" Grid.Column="0" Margin="5">Импорт JSON</Button>
        <Button Grid.Row="2" Grid.Column="1" Background="Aquamarine" Margin="5">Экспорт JSON</Button>
    </Grid>
</Window>
